<?xml version="1.0" encoding="UTF-8"?>
<aguila
   xmlns="http://www.pcraster.nl/pcrxml"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="http://www.pcraster.nl/pcrxml Aguila.xsd">
  <visualisationGroup>
    <cursorValueMonitorFile>monitorFile.xml</cursorValueMonitorFile>
    <searchSpace>
      <timesteps>
        <range>
          <begin>1</begin>
          <end>28</end>
          <increment>1</increment>
        </range>
        <dateMapper>
          <index>1</index>
          <timeOfIndex>1997-02-10T18:00:00</timeOfIndex>
          <duration>
           <hours>6</hours>
          </duration>
        </dateMapper>
      </timesteps>
    </searchSpace>
    <data>
     <name>upstreamArea.map</name>
     <drawProperties>
      <colourAssignment>
        <shiftedLogarithmic/>
      </colourAssignment>
     </drawProperties>
    </data>
    <view>
      <valueOnly>
        <!-- extra insertion of
             dem.map for order
          -->
        <item>dem.map</item>
        <item>ldd.map</item>
      </valueOnly>
    </view>
<!-- -->
    <view>
      <timeGraph>
        <item>rain.tss</item>
      </timeGraph>
    </view>
<!-- -->
    <view>
      <map>
        <item>runoff</item>
      </map>
    </view>
    <view>
      <timeGraph>
        <item>runoff</item>
        <item>runoff.tss</item>
      </timeGraph>
    </view>
    <view>
     <drape>
     <item>dem.map</item>
     <item>upstreamArea.map</item>
     </drape>
    </view>
  </visualisationGroup>
</aguila>
